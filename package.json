{
  "name": "clutch",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "bin": {
    "clutch": "./bin/clutch-cli.ts",
    "clutch-cli": "./bin/clutch-cli.ts"
  },
  "scripts": {
    "dev": "volta run node ./node_modules/next/dist/bin/next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "demo:up": "docker compose -f docker-compose.demo.yml --env-file .env.demo.local up -d",
    "demo:down": "docker compose -f docker-compose.demo.yml --env-file .env.demo.local down",
    "demo:reset": "tsx scripts/demo-reset.ts",
    "demo:clean": "tsx scripts/demo-reset.ts --force",
    "demo:deploy": "convex deploy --url http://localhost:3230 --yes",
    "demo:seed": "tsx scripts/seed-demo.ts",
    "demo:seed:clean": "tsx scripts/seed-demo.ts --clean",
    "demo:dev": "next dev -p 3002 --env-file .env.demo.local",
    "demo:safety-check": "tsx scripts/demo-safety-check.ts",
    "demo:screenshots": "tsx scripts/demo-screenshots.ts",
    "seed:prompts": "tsx scripts/seed-prompts.ts"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@tanstack/react-table": "^8.21.3",
    "@types/diff": "^8.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "diff": "^8.0.3",
    "framer-motion": "^12.33.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.3",
    "convex-test": "^0.0.41",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "volta": {
    "node": "22.22.0",
    "pnpm": "10.28.2"
  }
}
